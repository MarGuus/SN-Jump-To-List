{
    "name": "SN Jump To List",
    "description": "Jump to list view with a click or keyboard shortcut in servicenow",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting","tabs"],
    "action": {},
    "commands": {
      "_execute_action": {
        "suggested_key": "Alt+L"
      }
    },
    "icons": {
        "16": "/img/LIST16.png",
        "32": "/img/LIST32.png",
        "48": "/img/LIST48.png",
        "128": "/img/LIST128.png"
      }

}